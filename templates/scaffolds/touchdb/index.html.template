<div ng-app="<%= options.className %>Resource">
<h1>Listing <%= options.name %>s</h1>

<div ng-controller="<%= options.className %>Controller.index">
  <ul class="list">
    <li ng-click="goShow(<%= options.name %>._id)" ng-repeat="<%= options.name %> in <%= options.name %>Collection">
      {{<%= options.name %>.name}}
      <span class="chevron"/>
    </li>
  </ul>
</div>
